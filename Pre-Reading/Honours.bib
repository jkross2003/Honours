	doi = {10.3390/sym13030400},
	issn = {2073-8994},
	journal = {Symmetry},
	number = {3},
	title = {A Modern View of the Equation of State in Nuclear and Neutron Star Matter},
	url = {https://www.mdpi.com/2073-8994/13/3/400},
	volume = {13},
	year = {2021},
