	journal = {Annual Review of Nuclear and Particle Science},
	keywords = {pulsars},
	number = {Volume 71, 2021},
	pages = {433-464},
	publisher = {Annual Reviews},
	title = {Neutron Stars and the Nuclear Matter Equation of State},
